pipeline {
    agent any
    environment {
        path_to_file = "Neptun"
        file_name = "SSHMIDT"
        remote_dir_path = "Neptun"

    }
    stages {
        stage('git') {
            steps {
                git branch: 'main', credentialsId: 'GitHub_SSH_key', url: 'git@github.com:Salvadory/WebSite.git'
                sh 'pwd'
                sh 'ls -la'
            }
        }
        
       publishers {
        publishOverSsh {
            server('WEB_server') {
                credentials('user01') {
                    pathToKey('path01')
                }
                label('WEB_server')
                transferSet {
                    sourceFiles('files')
                    execCommand('command')
                }
            }
            server('WEB_server') {
                credentials('user2') {
                    key('key')
                }
                label('WEB_server')
                transferSet {
                    sourceFiles('files2')
                    execCommand('command2')
                }
            }
            parameterizedPublishing('PARAMETER')
        }
    }
        
        
        
        stage('Build') {
            steps {
                echo 'Building'
                
            }
        }

        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
    

        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}
