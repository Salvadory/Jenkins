pipeline {
    agent any

    stages {
        stage('git') {
            steps {
                git branch: 'main', credentialsId: 'GitHub_SSH_key', url: 'git@github.com:Salvadory/WebSite.git'
                sh 'pwd'
                sh 'ls -la'
            }
        }
        
        
        
        
        
        
        
        stage('Build') {
            steps {
                echo 'Building'
                
            }
        }

        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
    

        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}
