pipeline {
    agent any
    environment {
        path_to_file = "Neptun"
        file_name = "SSHMIDT"
        remote_dir_path = "Neptun"

    }
    stages {
        stage('git') {
            steps {
                git branch: 'main', credentialsId: 'GitHub_SSH_key', url: 'git@github.com:Salvadory/WebSite.git'
                sh 'pwd'
                sh 'ls -la'
            }
        }
        
        stage('example') {
            steps {
               publishOverSsh {
                    server('WEB_server') {
                        transferSet {
                             sourceFiles('index.html')
                 }
               }
             }
           }
         }
        
        
        
        
        
        stage('Build') {
            steps {
                echo 'Building'
                
            }
        }

        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
    

        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}
